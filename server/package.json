{
  "name": "em-server",
  "scripts": {
    "backup": "pm2 restart backup --cron-restart 0",
    "build": "rm -rf build ; npx tsc",
    "kill": "pm2 kill",
    "start": "npm run postdeploy ; pm2 start pm2.config.js",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "predeploy": "scripts/predeploy.sh",
    "postdeploy": "scripts/safe_postdeploy.sh",
    "postinstall": "patch-package",
    "restart": "pm2 restart pm2.config.js",
    "status": "pm2 status pm2.config.js",
    "stop": "pm2 delete backup ; pm2 stop pm2.config.js"
  },
  "dependencies": {
    "@hocuspocus/server": "^2.3.1",
    "level": "^6.0.1",
    "s3-cli": "https://github.com/raineorshine/node-s3-cli",
    "y-leveldb": "^0.1.2"
  },
  "devDependencies": {
    "@types/level": "^6.0.1",
    "patch-package": "^7.0.0",
    "pm2": "^5.3.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "yjs": "^13.5.29"
  },
  "overrides": {
    "graceful-fs": "^4.2.11"
  }
}
